<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	

   <!-- 订单服务 -->
   <dubbo:reference id="orderService"
        interface="com.facishare.pay.order.service.OrderService"
        version="1.0" 
        timeout="3000" 
        check="false"  /> 
        
   <dubbo:reference id="orderLogService"
        interface="com.facishare.pay.order.service.OrderLogService"
        version="1.0" 
        timeout="3000" 
        check="false"  /> 


    <dubbo:reference id="userWalletReadService"
                     interface="com.facishare.pay.bank.service.UserWalletReadService"
                     version="1.0"
                     timeout="3000"
                     check="false"/>
                     
    <dubbo:reference id="userWalletWriteService"
                     interface="com.facishare.pay.bank.service.UserWalletWriteService"
                     version="1.0"
                     timeout="3000"
                     check="false"/>
 <dubbo:reference id="sendMessageService"
                     interface="com.facishare.open.msg.service.SendMessageService"
                     version="1.0"
                     timeout="15000"
                     check="false"/>

    

</beans>
