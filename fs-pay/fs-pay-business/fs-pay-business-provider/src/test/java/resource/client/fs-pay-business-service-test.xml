<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
	
	 
  <!-- dubbo config  -->
    <dubbo:application name="fs-pay-business" />
<!--    <dubbo:registry protocol="zookeeper"  address="172.31.103.108:4180,172.31.103.109:4180,172.31.103.110:4180"/>  -->
<!--
   <dubbo:registry protocol="zookeeper"  address="172.31.102.102:2181" />
-->
    <dubbo:registry protocol="zookeeper"  address="172.31.105.107:4180" />
   <!-- <dubbo:registry protocol="zookeeper"  address="172.31.102.118:2181,172.31.102.118:3181,172.31.102.118:4181"/> --> 
    <dubbo:protocol name="dubbo" port="43003" />   
    <!-- 消费者角色   -->

    <dubbo:reference id="userCardInfoService" interface="com.facishare.pay.business.service.UserCardInfoService"
                     protocol="dubbo" timeout="15000" check="false" retries="0" version="1.0"/>

     <dubbo:reference id="userChargeService" interface="com.facishare.pay.business.service.UserChargeService"
                     protocol="dubbo" timeout="15000" check="false" retries="0" version="1.0"/>

     <dubbo:reference id="userGetMoneyService" interface="com.facishare.pay.business.service.UserGetMoneyService"
                     protocol="dubbo" timeout="15000" check="false" retries="0" version="1.0"/>

     <dubbo:reference id="userInfoService" interface="com.facishare.pay.business.service.UserInfoService"
                     protocol="dubbo" timeout="15000" check="false" retries="0" version="1.0"/>

    <dubbo:reference id="userTransferService" interface="com.facishare.pay.business.service.UserTransferService"
                     protocol="dubbo" timeout="15000" check="false" retries="0" version="1.0"/>
                     
    <dubbo:reference id="userWalletService" interface="com.facishare.pay.business.service.UserWalletService"
                     protocol="dubbo" timeout="15000" check="false" retries="0" version="1.0"/>



</beans>
